html
  head
    title OpenShift Pipeline
    link(rel="stylesheet" href="/static/stylesheets/bootstrap-4.0.0-beta.2.min.css")
  body
    main.container(role="main")
      div.row
        div.col-6 #
        div.col
            h1 test
        div.col
            h1 qa
        div.col
            h1 pilot
        div.col
            h1 prod
      each deploy in fooAppDeploys
        div.row
            div.col-6=deploy.commitHash + " " + deploy.commitMessage
            div.col deploy
            div.col deploy
            div.col deploy
            div.col deploy
